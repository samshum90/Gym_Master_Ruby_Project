<h1>Bookings</h1>

<p>
  <a href="/bookings/new">New booking</a>
</p>

<ul>
  <% for booking in @bookings %>
  <li>
  <p><%= booking.booking.name %></p>
    <p>
      <form action="/bookings/<%= booking.id %>/delete" method="POST">
        <input type="submit" value="delete" />
      </form>
    </p>
  </li>
  <% end %>
</ul>
